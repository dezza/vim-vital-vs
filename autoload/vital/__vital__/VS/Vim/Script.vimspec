let s:expect = themis#helper('expect')
let s:Script = vital#vital#import('VS.Vim.Script')

let s:root_dir = globpath(&runtimepath, 'autoload/vital/__vital__/VS')

Describe vital#__vital__#VS#Vim#Script

  Describe #call

    It should call external scripts
      call s:expect(s:Script.call(s:root_dir . '/Vim/Script.vim', '_spec', [1, 2])).to_equal(3)
    End

  End

End

